# API for touchstone package

## Exported functions

activate(head_ref = gert::git_branch(), base_ref = getOption("touchstone.default_base_ref", "main"), env = parent.frame())
benchmark_ls(name)
benchmark_read(name, ref)
benchmark_run_ref(expr_before_benchmark, ..., refs = c(ref_get_or_fail("GITHUB_BASE_REF"), ref_get_or_fail("GITHUB_HEAD_REF")), n = 100, path_pkg = ".")
benchmark_write(benchmark, name, ref, block = NA, iteration = NA, append = TRUE)
benchmarks_analyze(refs = c(ref_get_or_fail("GITHUB_BASE_REF"), ref_get_or_fail("GITHUB_HEAD_REF")), names = NULL, ci = 0.95)
deactivate(env = parent.frame())
dir_touchstone()
path_pinned_asset(..., ref = ref_get_or_fail("GITHUB_HEAD_REF"))
path_pr_comment()
pin_assets(..., ref = ref_get_or_fail("GITHUB_HEAD_REF"), overwrite = TRUE)
ref_get_or_fail(var)
refs_install(refs = c(ref_get_or_fail("GITHUB_BASE_REF"), ref_get_or_fail("GITHUB_HEAD_REF")), path_pkg = ".", install_dependencies = FALSE)
run_script(path = "touchstone/script.R", refs = c(ref_get_or_fail("GITHUB_BASE_REF"), ref_get_or_fail("GITHUB_HEAD_REF")))
touchstone_clear(all = FALSE)
use_touchstone(cancel = TRUE)
